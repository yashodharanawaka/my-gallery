{
  "name": "MyGalleryCodeBuildProject",
  "source": {
    "type": "GITHUB",
    "location": "https://github.com/yashodharanawaka/my-gallery",
    "gitCloneDepth": 1,
    "buildspec": "buildspec.yml"
  },
  "artifacts": {
    "type": "NO_ARTIFACTS"
  },
  "environment": {
    "type": "LINUX_CONTAINER",
    "image": "aws/codebuild/standard:4.0",
    "computeType": "BUILD_GENERAL1_SMALL",
    "environmentVariables": [
      {
        "name": "EXAMPLE_ENV_VAR",
        "value": "example-value",
        "type": "PLAINTEXT"
      }
    ]
  },
  "serviceRole": "arn:aws:iam::163156121837:role/service-role/your-codebuild-role"
}
